<div class="grid grid-cols-6">
  <div>Daily</div>
  <%= render partial: "shared/total", locals: { total: totals.total_annual_cost / 365 } %>
  <div>0</div>
  <div>0</div>
  <div>
    <%= humanized_money_with_symbol((income.total_net_income.fractional / 100) / 365 - totals.total_annual_cost / 365) %>
  </div>
</div>

